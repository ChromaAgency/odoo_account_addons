<odoo>
  <data>
    <record id="product_template_form_view_view_inherit_parallel_accounting" model="ir.ui.view">
      <field name="name">product.template.view.form.inherit</field>
      <field name="model">product.template</field>
      <field name="inherit_id" ref="account.product_template_form_view"/>
      <field name="arch" type="xml">
        <field name="property_account_income_id" position="after">
          <field name="property_income_secondary_account_id" groups="account.group_account_readonly"/>
        </field>
        <field name="property_account_expense_id" position="after">
          <field name="property_expenses_secondary_account_id" groups="account.group_account_readonly"/>
        </field>
      </field>
    </record>
  </data>
</odoo>
