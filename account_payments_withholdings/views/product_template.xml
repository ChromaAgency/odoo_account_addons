<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_product_template_inherit_payment_with_holdings" model="ir.ui.view">
        <field name="name">product.template.view.form.inherit</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_only_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='property_account_income_id']" position="after">
                    <field name="product_withholding_ids" widget="many2many_tags" string="Retenciones aplicables al producto" domain="[('type_tax_use','=','supplier')]"/>
            </xpath>
        </field>
    </record>
    

</odoo>
