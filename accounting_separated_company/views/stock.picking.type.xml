<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_picking_type_form_inherit_accounting_separated_company" model="ir.ui.view">
        <field name="name">stock.picking.type.view.form.inherit</field>
        <field name="model">stock.picking.type</field>
        <field name="inherit_id" ref="stock.view_picking_type_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='company_id']" position="after">
                <field name="accounting_company_stock_picking_type_ids" widget="many2many_tags" domain="[('company_id', '!=', company_id),('code', '=', code)]"/>
                
            </xpath>
        </field>
    </record>

</odoo>
